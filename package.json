{
  "name": "rollupjs.org",
  "author": "Rich Harris",
  "version": "0.1.0",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "compression": "^1.6.2",
    "cssnano": "^3.7.1",
    "eslint": "^3.0.1",
    "eslint-plugin-html": "^1.5.1",
    "eslint-plugin-import": "^2.2.0",
    "express": "^4.15.2",
    "gobble": "^0.11.3",
    "gobble-buble": "^0.13.0",
    "gobble-cli": "^0.7.0",
    "gobble-postcss": "^0.3.0",
    "gobble-replace": "^0.3.1",
    "gobble-rollup": "^0.34.0",
    "gobble-spelunk": "^0.6.0",
    "gobble-uglifyjs": "^0.2.1",
    "highlight.js": "^9.10.0",
    "marked": "^0.3.5",
    "postcss-clearfix": "^1.0.0",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-commonjs": "^3.1.0",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-svelte": "^1.6.0",
    "rollup-plugin-uglify": "^1.0.1",
    "sander": "^0.5.1",
    "surge": "^0.18.0",
    "svelte": "^1.12.1"
  },
  "scripts": {
    "start": "node server",
    "lint": "node node_modules/eslint/bin/eslint src --ext .js --ext .html",
    "build": "gobble build -f dist",
    "deploy": "surge dist rollupjs.org",
    "predeploy": "npm run update_overview && npm run build",
    "update_overview": "node scripts/update_overview",
    "bundle": "npm run bundle:main && npm run bundle:sw && npm run bundle:codemirror",
    "bundle:main": "rollup -c rollup/rollup.config.js",
    "bundle:sw": "rollup -c rollup/rollup.config.sw.js",
    "bundle:codemirror": "rollup -c rollup/rollup.config.codemirror.js",
    "rollup:dev": "export DEV=true && rollup -c rollup/rollup.config.js -w",
    "dev": "npm run rollup:dev & ( export DEV=true && node scripts/prep && node server )",
    "prep": "node scripts/prep && npm run bundle"
  },
  "dependencies": {
    "codemirror": "^5.16.0"
  }
}
